{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "module": "NodeNext",
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "types": [
      "jest",
      "node"
    ],
    "isolatedModules": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "app/**/*.ts",
    "app/**/*.tsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "components/**/*.ts",
    "components/**/*.tsx",
    "hooks/**/*.ts",
    "hooks/**/*.tsx",
    "lib/**/*.ts",
    "lib/**/*.tsx",
    "libs/**/*.ts",
    "libs/**/*.tsx",
    "pages/**/*.ts",
    "pages/**/*.tsx",
    "scripts/**/*.ts",
    "scripts/**/*.tsx",
    ".next/dev/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/share-availability/page.js",
    "app/profile/page.js"
  ],
  "exclude": [
    "node_modules",
    ".next"
  ]
}