{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/{page,route,layout,template,not-found,error,global-error}.{js,jsx,ts,tsx}!",
    "app/actions/**/*.{js,jsx,ts,tsx}!",
    "components/**/*.{js,jsx,ts,tsx}!",
    "hooks/**/*.{js,jsx,ts,tsx}!",
    "lib/**/*.{js,jsx,ts,tsx}!",
    "libs/**/*.{js,jsx,ts,tsx}!",
    "types/**/*.ts!",
    "next-sitemap.config.js",
    "config.js!",
    "next-sitemap.config.js"
  ],
  "project": ["**/*.{js,jsx,ts,tsx,css}"],
  "ignore": [
    "scripts/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.integration.ts",
    "**/*.test.tsx",
    "**/*.spec.tsx",
    "**/*.integration.tsx"
  ],
  "ignoreBinaries": ["tsc"],
  "tags": ["lintignore"]
}
